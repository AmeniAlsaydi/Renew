# platform :ios, '9.0'

use_frameworks!
inhibit_all_warnings!

def project_pods
    # Pods for Renew
  pod 'Firebase/Auth'
  pod 'Firebase/Analytics'
  pod 'Firebase/Firestore'
  pod 'FirebaseFirestoreSwift'
  pod 'Firebase/Storage'
  pod 'Kingfisher'
  pod 'MessageKit'

end


target 'Renew' do
  project_pods

end


target 'RenewTests' do
  project_pods

end
